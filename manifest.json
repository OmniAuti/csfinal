{
    "name": "Dead Cat",
    "description": "Dice rolling with four, six, eight, twelve, adn twenty, sided dice. Ability to add modifiers and roll more than one dice",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["none"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "dice.png",
        "32": "dice.png",
        "48": "dice.png",
        "128": "dice.png"
      }
    },
    "icons": {
      "16": "dice.png",
      "32": "dice.png",
      "48": "dice.png",
      "128": "dice.png"
    }
  }